{
	"files.associations": {
		"**/Scripts/**/*.txt": "rsdk",
		"*.txt": "plaintext"
	},
	"editor.lineNumbers": "on",
	"search.showLineNumbers": true,
	"vscord.status.details.text.viewing": "In {workspace} (Sonic Forever Mod)",
	"vscord.status.resetElapsedTimePerFile": true,
	"vscord.status.idle.disconnectOnIdle": true,
	"vscord.app.name": "Visual Studio Code",
	"vscord.status.problems.enabled": false,
	"vscord.status.state.text.editing": "Working on {file_name}{file_extension}\n{current_line}",
	"files.exclude": {
		"**/.git": false
	}
}